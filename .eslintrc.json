{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "@typescript-eslint/no-unused-vars": [
      "error",
      {
        "varsIgnorePattern": "^(increment|decrement|get|copy).*",
        "argsIgnorePattern": "^_",
        "vars": "all",
        "args": "none"
      }
    ]
  }
}
